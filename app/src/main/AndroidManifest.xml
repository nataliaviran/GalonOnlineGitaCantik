<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.gaol">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home"></activity>
        <activity android:name=".HistoryFragment" />
        <activity android:name=".UserFragment" />
        <activity android:name=".NotificationFragment" />
        <activity android:name=".HomeFragment" />
        <activity
            android:name=".MainBottom"
            android:label="@string/title_activity_main_bottom" />
        <activity android:name=".support.information" />
        <activity android:name=".HomeMenu" />
        <activity android:name=".login.LogIn" />
        <activity android:name=".register.Register" />
        <activity android:name=".splash.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>